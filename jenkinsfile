node {
    stage('Build') {
        sh 'echo "Building......"'
    }
    
    stage('deploy to x'){
        echo "Deploying....."
    }
    
    stage('Post-Build') {
        try {
            sh 'echo "Esto es un script personalizado"'
            bat 'echo "Esto es otro script personalizado"'

        } catch (Exception e) {
            // Manejo de excepciones, si es necesario
            // ...
            // ...
        } finally {
            // Acciones a ejecutar siempre, independientemente del resultado de la construcci√≥n
            // ...
            // ...
            jiraSendBuildInfo branch: 'PRUEB-2', site: 'testbpop.atlassian.net'
        }
    }
}
