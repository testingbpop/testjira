pipeline {
     agent any
     stages {
         stage('Build') {
             steps {
                 echo 'Building....'
             }
             post {
                 always {
                     echo "post numero 1......"
                 }
             }
         }
        stage('Deploy to Production') {
            steps {
                echo 'Deploying to Production from master.......'
            }
            post {
                always {
                    echo "post numero 2......"
                }
            }
     }
     }
 }
