#!/usr/bin/env groovy

node (''){

    def error = null
    def registry = null
    def configDeploy =null
    def branchedVersion = null

    try {
        construccion()
    }  
    catch(caughtError) {
        echo 'hola'
    }

    finally {
        echo 'se termino, no va mas'
    }
}

def construccion(){
    stage('Build'){
        echo "Building....."
    }
}

def deploy(){
    stage('deploy to x'){
        echo "Deploying....."
    }
}
